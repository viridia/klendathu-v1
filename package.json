{
  "name": "klendathu",
  "version": "0.0.1",
  "description": "Flexible bug tracking and agile task management application.",
  "main": "server/index.js",
  "scripts": {
    "client": "node client/devServer.js",
    "install": "webpack",
    "start": "supervisor -w server server",
    "start-pm2": "pm2 --node-args=\"--harmony\" start process.json",
    "stop": "pm2 kill",
    "debug": "node --inspect --harmony server",
    "build-container": "docker build -t viridia/klendathu .",
    "start-container": "docker run --rm -it -u app -p 49160:8080 --name klendathu -v $(pwd):/usr/src/app viridia/klendathu",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test-server": "mocha -w server/**/*.tests.js"
  },
  "keywords": [
    "bug",
    "tracking",
    "express",
    "react"
  ],
  "author": "Talin <viridia@gmail.com>",
