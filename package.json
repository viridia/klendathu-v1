{
  "name": "klendathu",
  "version": "0.0.1",
  "description": "Flexible bug tracking and agile task management application.",
  "main": "server/index.js",
  "scripts": {
    "client": "cd client && webpack-dev-server --inline --port 8081",
    "client-build": "cd client && webpack",
    "start": "pm2 --node-args=\"--harmony\" start process.json",
    "stop": "pm2 kill",
    "build-container": "docker build -t viridia/klendathu .",
    "start-container": "docker run --rm -it -u app -p 49160:8080 --name klendathu -v $(pwd):/usr/src/app viridia/klendathu",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "bug",
    "tracking",
    "express",
    "react"
  ],
  "author": "Talin <viridia@gmail.com>",
